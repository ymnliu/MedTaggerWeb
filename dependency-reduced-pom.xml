    <dependency>
      <groupId>org.jetbrains</groupId>
      <artifactId>annotations</artifactId>
      <version>13.0</version>
      <scope>provided</scope>
    </dependency>
  </dependencies>
  <properties>
    <uimafit.version>2.4.0</uimafit.version>
    <uimaj.version>2.10.0</uimaj.version>
  </properties>
</project>

