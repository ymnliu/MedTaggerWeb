+
+.span rect.MissingAnnotation + text {
+  opacity: 0.5;
+}
+
+.span rect.ChangedAnnotation {
+  stroke: #ffff99;
+  stroke-width: 2;
+}
+
+.shadow_ChangedAnnotation {
+  fill: #ff4141;
